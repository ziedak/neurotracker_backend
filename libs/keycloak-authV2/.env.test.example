# Environment Variables for Integration Tests
# Copy to .env.test and update values as needed

# Keycloak Configuration
KEYCLOAK_SERVER_URL=http://localhost:8080
KEYCLOAK_REALM=test-realm
KEYCLOAK_CLIENT_ID=test-client
KEYCLOAK_CLIENT_SECRET=test-secret

# Database Configuration
DATABASE_URL=postgresql://postgres:TEST@localhost:5432/neurotracker

# Redis Configuration (optional - for caching tests)
REDIS_URL=redis://localhost:6379

# Test Configuration
TEST_TIMEOUT=30000
CLEANUP_AFTER_TESTS=true
LOG_LEVEL=info

# Docker Compose Configuration (for reference)
# Keycloak Admin: http://localhost:8080/admin
# Username: admin
# Password: admin

# PostgreSQL Access:
# Host: localhost
# Port: 5432
# Database: keycloak_test
# Username: test
# Password: test

# Redis Access (if enabled):
# Host: localhost
# Port: 6379
# Password: (none)
